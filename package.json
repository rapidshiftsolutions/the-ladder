  "name": "newport-pitstop",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "dev:stable": "NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:clean": "node scripts/clear-cache.js && node scripts/dev.js",
    "dev:reset": "rm -rf .next && node scripts/clear-cache.js && node scripts/dev.js",
    "dev:prod": "NODE_ENV=production next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typegen": "sanity schema extract --path=src/sanity/extract.json && sanity typegen generate && rm ./src/sanity/extract.json",
    "clear-cache": "node scripts/clear-cache.js",
    "clear-sw": "echo 'Clearing service workers and cache' && touch public/force-clear-sw.txt",
    "generate-pwa-icons": "node scripts/generate-pwa-icons.js",
    "generate-screenshots": "node scripts/generate-screenshots.js",
    "pwa-setup": "npm run generate-pwa-icons && npm run generate-screenshots",
    "analyze": "ANALYZE=true npm run build",
    "bundle-analyzer": "npx @next/bundle-analyzer"
  },
